programa {
  funcao inicio() {
    real s1, s2, s3, s4, s5, media

    escreva("Informe o 1° salário: ")
    leia(s1)
    escreva("Informe o 2° salário: ")
    leia(s2)
    escreva("Informe o 3° salário: ")
    leia(s3)
    escreva("Informe o 4° salário: ")
    leia(s4)
    escreva("Informe o 5° salário: ")
    leia(s5)

    media = (s1 + s2 + s3 + s4 + s5) / 5

    escreva("A média salarial do setor de TI é: ", media)


  }
}
