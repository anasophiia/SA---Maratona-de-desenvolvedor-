programa {
  funcao inicio() {
    real idade, tempo

    escreva("Informe sua idade: ")
    leia(idade)
    escreva("Informe o seu tempo de contribuição:")
    leia(tempo)

    se(idade >= 60 ou tempo >= 30)
    escreva("Pode se aposentar")
    senao
    escreva("Não pode se aposentar")

  }
  }
}
